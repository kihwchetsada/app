class animal{
  void makeSound(){
    print("animal make a sound");
  }
}
class dog extends animal{
  void bark(){
    print('Dog bark');
  }
}
class bird extends animal{
void fly(){
  print('bird fly');
}
  void makeSound(){
    print('dog make a sound');
    print('bird make a sound');
  }

void main() {
  dog mydog = dog();
  mydog.makeSound();
  mydog.bark();
  bird mybird = bird();
  mybird.makeSound();
  mybird.fly();
}
